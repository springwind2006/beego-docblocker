{
    "name": "beego-docblocker",
    "displayName": "Beego DocBlocker",
    "description": "A simple, dependency free Beego specific DocBlocking package",
    "keywords": ["vscode", "plugin", "beego", "golang"],
    "version": "0.0.1",
    "publisher": "steeze",
    "engines": {
        "vscode": "^1.30.0"
    },
    "repository": {
		"type": "git",
		"url": "https://github.com/springwind2006/beego-docblocker.git"
	},
    "categories": [
        "Other"
    ],
    "icon": "images/beego_icon.png",
    "activationEvents": [
        "onLanguage:go"
    ],
    "main": "./src/extension",
    "contributes": {
        "configuration": {
			"type": "object",
			"title": "Beego DocBlocker configuration",
			"properties": {
				"beego-docblocker.author": {
					"type": "object",
					"default": {
						"name": "Name",
						"email": "email@email.com"
					},
					"description": "Default author tag"
				}
			}
		}
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
